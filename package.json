{
  "name": "@motioneffector/llm",
  "version": "0.1.0",
  "description": "TypeScript client for LLM APIs with OpenRouter support, streaming, conversation management, and automatic retries",
  "author": "motioneffector",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/motioneffector/llm"
  },
  "homepage": "https://github.com/motioneffector/llm#readme",
  "bugs": {
    "url": "https://github.com/motioneffector/llm/issues"
  },
  "keywords": [
    "llm",
    "openrouter",
    "claude",
    "gpt",
    "ai",
    "chat",
    "streaming",
    "conversation",
    "typescript",
    "anthropic",
    "openai"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "test:run": "vitest run",
    "fuzz:thorough": "FUZZ_THOROUGH=1 vitest run src/fuzz.test.ts --testTimeout=30000",
    "lint": "eslint src",
    "format": "prettier --write src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run test:run && npm run build"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.0.0",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.4.0",
    "vitest": "^2.1.8"
  }
}
